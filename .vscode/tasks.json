{
	"version": "2.0.0",
	"tasks": [
        {
            "label": "Build",
            "type": "shell",
            "command": "make",
            "options": {
                "cwd": "${fileDirname}"
            },
            "group": {
                "kind": "build",
                "isDefault": true
            },
        },
        {
            "label": "Umount",
            "type": "shell",
            "command": "umount",
            "args": [
                "${fileDirname}/mnt",
            ]
        },
    ],
}
